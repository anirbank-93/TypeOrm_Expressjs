{
  "name": "3",
  "version": "1.0.0",
  "description": "A basic Node.js-Express.js-TypeScript setup",
  "main": "./dist/src/index.js",
  "scripts": {
    "build": "tsc",
    "start": "nodemon"
  },
  "keywords": [
    "typescript",
    "template"
  ],
  "license": "MIT",
  "nyc": {
    "cache": false,
    "check-coverage": false,
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "coverage/**",
      "src/test/**",
      "src/config/environments",
      "src/lib/interfaces",
      "node_modules/**"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": false,
    "reporter": [
      "html",
      "text-summary"
    ],
    "all": true,
    "instrument": true
  },
  "dependencies": {
    "ajv": "^8.16.0",
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.2",
    "class-validator": "^0.14.1",
    "core-decorators": "^0.20.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-winston": "^4.2.0",
    "helmet": "^7.1.0",
    "lodash": "^4.17.21",
    "mysql2": "^3.10.2",
    "reflect-metadata": "^0.2.2",
    "tslint-sonarts": "^1.9.0",
    "typeorm": "^0.3.20",
    "typescript": "^5.5.3",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/bluebird": "^3.5.42",
    "@types/chai": "^4.3.16",
    "@types/express": "^4.17.21",
    "@types/mocha": "^10.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.10",
    "@types/validator": "^13.12.0",
    "chai": "^5.1.1",
    "chai-as-promised": "^8.0.0",
    "chai-http": "^5.0.0",
    "chai-spies": "^1.1.0",
    "concurrently": "^8.2.2",
    "istanbul": "^0.4.5",
    "mocha": "^10.6.0",
    "nock": "^13.5.4",
    "nodemon": "^3.1.4",
    "nyc": "^17.0.0",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "ts-sinon": "^2.0.2"
  }
}
